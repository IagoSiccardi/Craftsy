<!DOCTYPE html>
<html lang="en">

    <%- include('./partials/head', {
        title : "Login"
    }) %> 

<body class="register">
    <%- include('./partials/header') %> 
    <main>
        <section>
            <h1>Ingresa</h1>
            <hr>
            <form>

                
                
                <div class="register__form-box">
                    <label for="email">Email</label>
                    <input class="register__form-input" type="email" id="email" name="email" placeholder="Escribí tu email">
                     <span class="register__text-error">Obligatorio</span>
                </div>
                <div class="register__form-box">
                    <label for="password">Contraseña</label>
                    <input class="register__form-input" type="password" id="password" name="password" placeholder="Escribí tu contraseña">
                     <span class="register__text-error">Obligatorio</span>
                </div>
               

                <div class="register__form-box register__center">
                    
                    <input type="checkbox" name="recorda" id="recorda">
                    <label for="recorda">Recordar contraseña</label>
                </div>
                <div class="register__form-box register__center">
                    
                    
                    <button type="submit">Ingresa</button>
                    
                </div>
                
            </form>
        </section>

    </main>
    <%- include('./partials/footer') %> 
</body>

</html>